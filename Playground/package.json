{
  "name": "playground-node-express-bookshelf-angular2",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run tsc:w:server\" \"npm run tsc:w:client\" \"npm run lite\" \"nodemon server/server.js\" ",
    "tsc": "tsc",
    "tsc:w:server": "tsc -w -p ./server",
    "tsc:w:client": "tsc -w -p ./client",
    "lite": "lite-server -c client/bs-config.json",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "license": "ISC",
  "dependencies": {
    "angular2": "2.0.0-beta.13",
    "body-parser": "~1.13.2",
    "bookshelf": "^0.9.4",
    "co": "^4.6.0",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "es6-shim": "^0.35.0",
    "express": "~4.13.1",
    "knex": "^0.10.0",
    "morgan": "~1.6.1",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "serve-favicon": "~2.3.0",
    "systemjs": "0.19.25",
    "zone.js": "0.6.6"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.1.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.11"
  }
}
